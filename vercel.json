{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.php",
            "use": "vercel-php@0.7.4"
        }
    ],
    "functions": {
        "api/**/*.php": {
            "runtime": "vercel-php@0.7.4",
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/assets/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "dest": "/assets/$1"
        },
        {
            "src": "/",
            "dest": "/api/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/$1"
        }
    ],
    "github": {
        "silent": true
    }
}